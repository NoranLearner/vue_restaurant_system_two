{"ast":null,"code":"import Navbar from '@/components/header/Navbar.vue';\nimport { mapActions } from 'vuex';\nimport axios from 'axios';\nexport default {\n  name: 'DeleteLocation',\n  data() {\n    return {\n      deleteLocation: '',\n      userId: \"\",\n      name: '',\n      phone: '',\n      address: '',\n      locationData: [],\n      successMessage: '',\n      errorMessage: ''\n    };\n  },\n  mounted() {\n    let user = localStorage.getItem(\"user_info\");\n    if (user) {\n      this.deleteLocation = this.$route.params.locationId;\n      this.userId = JSON.parse(user).id;\n      this.canCurrentUserAccessThisLocation();\n    } else {\n      // Redirect to Sign Up page\n      this.redirectTo({\n        val: 'signup'\n      });\n    }\n  },\n  components: {\n    Navbar\n  },\n  methods: {\n    ...mapActions(['redirectTo']),\n    // Redirect to Home Page\n    goBack() {\n      this.redirectTo({\n        val: \"home\"\n      });\n    },\n    async canCurrentUserAccessThisLocation() {\n      let result = await axios.get(`http://localhost:3000/locations?id=${this.deleteLocation}&userId=${this.userId}`);\n      if (result.status == 200 && result.data.length > 0) {\n        this.locationData = result.data;\n        this.name = this.locationData[0].name;\n        this.phone = this.locationData[0].phone;\n        this.address = this.locationData[0].address;\n      } else {\n        this.redirectTo({\n          val: \"home\"\n        });\n      }\n    },\n    async deleteRestaurant() {\n      let result = await axios.delete(`http://localhost:3000/locations/${this.deleteLocation}`);\n      if (result.status == 200) {\n        // Show Success Message\n        this.successMessage = 'Delete Location Successfully';\n        this.errorMessage = '';\n        setTimeout(() => {\n          this.redirectTo({\n            val: \"home\"\n          });\n        }, 2000);\n      } else {\n        // Show Error Message\n        this.successMessage = '';\n        this.errorMessage = 'Something went wrong, Try Again';\n      }\n    }\n  }\n};","map":{"version":3,"names":["Navbar","mapActions","axios","name","data","deleteLocation","userId","phone","address","locationData","successMessage","errorMessage","mounted","user","localStorage","getItem","$route","params","locationId","JSON","parse","id","canCurrentUserAccessThisLocation","redirectTo","val","components","methods","goBack","result","get","status","length","deleteRestaurant","delete","setTimeout"],"sources":["/opt/lampp/htdocs/rest-sys-2021/src/components/locations/DeleteLocation.vue"],"sourcesContent":["<template>\n\n    <Navbar />\n\n    <form @click.prevent>\n\n        <div class=\"row g-3 align-items-center\">\n            <div class=\"col-auto d-block mx-auto\">\n                <h1 class=\"fs-5 text-center mb-3 text-danger\">Delete Restaurant #{{ deleteLocation }}</h1>\n                <p class=\"text-danger fw-bolder\">Are you sure you want to delete this location?</p>\n                <div class=\"text-center\">\n                    {{ name }}\n                    <br />\n                    {{ phone }}\n                    <br />\n                    {{ address }}\n                </div>\n                <hr />\n            </div>\n        </div>\n\n        <div class=\"row g-3 align-items-center\">\n            <div class=\"col-auto d-block mx-auto\">\n                <button type=\"button\" class=\"btn btn-info text-white\" @click=\"goBack()\">Go Back</button>\n                &nbsp;\n                <button type=\"button\" class=\"btn btn-danger text-white\" @click=\"deleteRestaurant()\">Delete Now</button>\n            </div>\n        </div>\n\n        <br />\n\n        <div class=\"row g-3 align-items-center\">\n            <div class=\"col-auto d-block mx-auto alert alert-success\" v-if=\"successMessage.length > 0\">\n                {{ successMessage }}\n            </div>\n            <div class=\"col-auto d-block mx-auto alert alert-danger\" v-if=\"errorMessage.length > 0\">\n                {{ errorMessage }}\n            </div>\n        </div>\n\n    </form>\n\n</template>\n\n<script>\n\nimport Navbar from '@/components/header/Navbar.vue';\n\nimport { mapActions } from 'vuex';\n\nimport axios from 'axios';\n\nexport default {\n\n    name: 'DeleteLocation',\n\n    data() {\n        return {\n            deleteLocation: '',\n            userId: \"\",\n            name: '',\n            phone: '',\n            address: '',\n            locationData: [],\n            successMessage: '',\n            errorMessage: '',\n        }\n    },\n\n    mounted() {\n        let user = localStorage.getItem(\"user_info\");\n        if (user) {\n            this.deleteLocation = this.$route.params.locationId;\n            this.userId = JSON.parse(user).id;\n            this.canCurrentUserAccessThisLocation();\n        }\n        else {\n            // Redirect to Sign Up page\n            this.redirectTo({ val: 'signup' });\n        }\n    },\n\n    components: {\n        Navbar,\n    },\n\n    methods: {\n        ...mapActions(['redirectTo']),\n        // Redirect to Home Page\n        goBack() {\n            this.redirectTo({ val: \"home\" });\n        },\n        async canCurrentUserAccessThisLocation() {\n            let result = await axios.get(`http://localhost:3000/locations?id=${this.deleteLocation}&userId=${this.userId}`);\n            if (result.status == 200 && result.data.length > 0) {\n                this.locationData = result.data;\n                this.name = this.locationData[0].name;\n                this.phone = this.locationData[0].phone;\n                this.address = this.locationData[0].address;\n            } else {\n                this.redirectTo({ val: \"home\" });\n            }\n        },\n        async deleteRestaurant() {\n            let result = await axios.delete(`http://localhost:3000/locations/${this.deleteLocation}`);\n            if (result.status == 200) {\n                // Show Success Message\n                this.successMessage = 'Delete Location Successfully';\n                this.errorMessage = '';\n                setTimeout(() => {\n                    this.redirectTo({ val: \"home\" });\n                }, 2000);\n            } else {\n                // Show Error Message\n                this.successMessage = '';\n                this.errorMessage = 'Something went wrong, Try Again';\n            }\n        },\n    },\n\n}\n\n</script>\n\n<style scoped lang=\"scss\"></style>"],"mappings":"AA8CA,OAAOA,MAAK,MAAO,gCAAgC;AAEnD,SAASC,UAAS,QAAS,MAAM;AAEjC,OAAOC,KAAI,MAAO,OAAO;AAEzB,eAAe;EAEXC,IAAI,EAAE,gBAAgB;EAEtBC,IAAIA,CAAA,EAAG;IACH,OAAO;MACHC,cAAc,EAAE,EAAE;MAClBC,MAAM,EAAE,EAAE;MACVH,IAAI,EAAE,EAAE;MACRI,KAAK,EAAE,EAAE;MACTC,OAAO,EAAE,EAAE;MACXC,YAAY,EAAE,EAAE;MAChBC,cAAc,EAAE,EAAE;MAClBC,YAAY,EAAE;IAClB;EACJ,CAAC;EAEDC,OAAOA,CAAA,EAAG;IACN,IAAIC,IAAG,GAAIC,YAAY,CAACC,OAAO,CAAC,WAAW,CAAC;IAC5C,IAAIF,IAAI,EAAE;MACN,IAAI,CAACR,cAAa,GAAI,IAAI,CAACW,MAAM,CAACC,MAAM,CAACC,UAAU;MACnD,IAAI,CAACZ,MAAK,GAAIa,IAAI,CAACC,KAAK,CAACP,IAAI,CAAC,CAACQ,EAAE;MACjC,IAAI,CAACC,gCAAgC,CAAC,CAAC;IAC3C,OACK;MACD;MACA,IAAI,CAACC,UAAU,CAAC;QAAEC,GAAG,EAAE;MAAS,CAAC,CAAC;IACtC;EACJ,CAAC;EAEDC,UAAU,EAAE;IACRzB;EACJ,CAAC;EAED0B,OAAO,EAAE;IACL,GAAGzB,UAAU,CAAC,CAAC,YAAY,CAAC,CAAC;IAC7B;IACA0B,MAAMA,CAAA,EAAG;MACL,IAAI,CAACJ,UAAU,CAAC;QAAEC,GAAG,EAAE;MAAO,CAAC,CAAC;IACpC,CAAC;IACD,MAAMF,gCAAgCA,CAAA,EAAG;MACrC,IAAIM,MAAK,GAAI,MAAM1B,KAAK,CAAC2B,GAAG,CAAE,sCAAqC,IAAI,CAACxB,cAAe,WAAU,IAAI,CAACC,MAAO,EAAC,CAAC;MAC/G,IAAIsB,MAAM,CAACE,MAAK,IAAK,GAAE,IAAKF,MAAM,CAACxB,IAAI,CAAC2B,MAAK,GAAI,CAAC,EAAE;QAChD,IAAI,CAACtB,YAAW,GAAImB,MAAM,CAACxB,IAAI;QAC/B,IAAI,CAACD,IAAG,GAAI,IAAI,CAACM,YAAY,CAAC,CAAC,CAAC,CAACN,IAAI;QACrC,IAAI,CAACI,KAAI,GAAI,IAAI,CAACE,YAAY,CAAC,CAAC,CAAC,CAACF,KAAK;QACvC,IAAI,CAACC,OAAM,GAAI,IAAI,CAACC,YAAY,CAAC,CAAC,CAAC,CAACD,OAAO;MAC/C,OAAO;QACH,IAAI,CAACe,UAAU,CAAC;UAAEC,GAAG,EAAE;QAAO,CAAC,CAAC;MACpC;IACJ,CAAC;IACD,MAAMQ,gBAAgBA,CAAA,EAAG;MACrB,IAAIJ,MAAK,GAAI,MAAM1B,KA