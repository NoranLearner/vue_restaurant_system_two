eleteCategory() {\n            let result = await axios.delete(`http://localhost:3000/categories/${this.catId}`);\n            let allResults = [];\n            for (var i = 0; i < this.allItemsIdIs.length; i++){\n                let result = await axios.delete(`http://localhost:3000/items/${this.allItemsIdIs[i]}`);\n                if (result.status == 200) {\n                    allResults.push(true);\n                } else {\n                    allResults.push(false);\n                }\n            }\n            if (result.status == 200 && !allResults.includes(false)) {\n                this.successMessage = 'Delete Category and Related Items Successfully';\n                this.errorMessage = '';\n                setTimeout(() => {\n                    this.$router.push({ name: \"view-categories\", params: { locationId: this.locId } });\n                }, 2000);\n            } else {\n                this.successMessage = '';\n                this.errorMessage = 'Something went wrong, Try again';\n            }\n        },\n\n    },\n\n}\n</script>\n\n<style lang=\"scss\" scoped></style>"],"mappings":";;EAESA,KAAK,EAAC;AAAW;;EAQTA,KAAK,EAAC;AAA4B;;EAC9BA,KAAK,EAAC;AAAsC;gCAG7CC,mBAAA,CAAI;gCACJA,mBAAA,CAEI;EAFDD,KAAK,EAAC;AAAa,GAAC,mDAEvB;gCAIRC,mBAAA,CAMM;EANDD,KAAK,EAAC;AAA4B,I,aACnCC,mBAAA,CAIM;EAJDD,KAAK,EAAC;AAA0B,I,aACjCE,mBAAA,oFAAmF,E,aAvBvGC,gBAAA,CAuBuG,OAEnF,G,aAAAD,mBAAA,iGAAgG,C;gCAIxGD,mBAAA,CAAM;;EAEDD,KAAK,EAAC;AAA4B;;EA/BnDI,GAAA;EAgCqBJ,KAAK,EAAC;;;EAhC3BI,GAAA;EAmCqBJ,KAAK,EAAC;;;;uBAjCvBK,mBAAA,CAwCM,OAxCNC,UAwCM,GAtCFC,YAAA,CAAUC,iBAAA,GAEVN,mBAAA,kCAAqC,EAErCD,mBAAA,CAgCO;IAhCAQ,OAAK,EAAAC,MAAA,QAAAA,MAAA,MARpBC,cAAA,CAQc,QAAc;MAEhBV,mBAAA,CASM,OATNW,UASM,GARFX,mBAAA,CAOM,OAPNY,UAOM,GANFZ,mBAAA,CAAqC,YAAjC,mBAAiB,GAAAa,gBAAA,CAAGC,KAAA,CAAAC,KAAK,kBAC7Bf,mBAAA,CAAkC,YAA9B,iBAAe,GAAAa,gBAAA,CAAGC,KAAA,CAAAE,IAAI,kBAC1BC,UAAI,EACJC,UAEI,C,KAIZC,UAMM,EAENC,UAAM,EAENpB,mBAAA,CAOM,OAPNqB,UAOM,GAN8DP,KAAA,CAAAQ,cAAc,CAACC,MAAM,Q,cAArFnB,mBAA