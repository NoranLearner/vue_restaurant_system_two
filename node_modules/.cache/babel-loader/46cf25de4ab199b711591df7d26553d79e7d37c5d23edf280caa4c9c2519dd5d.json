{"ast":null,"code":"import Navbar from '@/components/header/Navbar.vue';\nimport { mapActions } from 'vuex';\nimport axios from 'axios';\nimport UserLocations from '@/components/locations/UserLocations.vue';\nexport default {\n  name: 'HomeView',\n  data() {\n    return {\n      userName: '',\n      listOfLocations: [],\n      userId: \"\"\n    };\n  },\n  async mounted() {\n    let user = localStorage.getItem(\"user_info\");\n    if (!user) {\n      // Redirect to Home page\n      this.redirectTo({\n        val: 'signup'\n      });\n    } else {\n      this.userName = JSON.parse(user).name;\n      this.userId = JSON.parse(user).id;\n    }\n    let result = await axios.get(`http://localhost:3000/locations?userId=${this.userId}`);\n    if (result.status == 200 && result.data.length > 0) {\n      console.log(result.data);\n      this.listOfLocations = result.data;\n    }\n  },\n  components: {\n    Navbar,\n    UserLocations\n  },\n  methods: {\n    ...mapActions(['redirectTo'])\n  }\n};","map":{"version":3,"names":["Navbar","mapActions","axios","UserLocations","name","data","userName","listOfLocations","userId","mounted","user","localStorage","getItem","redirectTo","val","JSON","parse","id","result","get","status","length","console","log","components","methods"],"sources":["/opt/lampp/htdocs/rest-sys-2021/src/views/HomeView.vue"],"sourcesContent":["<template>\n\n  <Navbar />\n\n  <div class=\"home container\">\n\n    <h1>This is an home page</h1>\n\n    <p class=\"text-start\">\n      Welcome:\n      <span class=\"text-primary\">{{ userName }}</span>\n      <router-link :to=\"{ name: 'profile' }\" class=\"float-end\">\n        <button type=\"button\" class=\"btn btn-primary\">Profile</button>\n      </router-link>\n    </p>\n\n    <router-link :to=\"{ name: 'add-new-location' }\">\n      <button type=\"button\" class=\"btn btn-primary\">\n        Add New Restaurant\n      </button>\n    </router-link>\n\n    <UserLocations :allLocations=\"listOfLocations\" />\n\n  </div>\n\n</template>\n\n<script>\n\nimport Navbar from '@/components/header/Navbar.vue';\n\nimport { mapActions } from 'vuex';\n\nimport axios from 'axios';\n\nimport UserLocations from '@/components/locations/UserLocations.vue';\n\nexport default {\n\n  name: 'HomeView',\n\n  data() {\n    return {\n      userName: '',\n      listOfLocations: [],\n      userId: \"\",\n    }\n  },\n\n  async mounted() {\n    let user = localStorage.getItem(\"user_info\");\n    if (!user) {\n      // Redirect to Home page\n      this.redirectTo({ val: 'signup' });\n    } else {\n      this.userName = JSON.parse(user).name;\n      this.userId = JSON.parse(user).id;\n    }\n    let result = await axios.get(`http://localhost:3000/locations?userId=${this.userId}`);\n    if (result.status == 200 && result.data.length > 0) {\n      console.log(result.data);\n      this.listOfLocations = result.data;\n    }\n  },\n\n  components: {\n    Navbar,\n    UserLocations,\n  },\n\n  methods: {\n    ...mapActions(['redirectTo']),\n  },\n\n}\n\n</script>\n\n<style lang=\"scss\"></style>"],"mappings":"AA8BA,OAAOA,MAAK,MAAO,gCAAgC;AAEnD,SAASC,UAAS,QAAS,MAAM;AAEjC,OAAOC,KAAI,MAAO,OAAO;AAEzB,OAAOC,aAAY,MAAO,0CAA0C;AAEpE,eAAe;EAEbC,IAAI,EAAE,UAAU;EAEhBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,QAAQ,EAAE,EAAE;MACZC,eAAe,EAAE,EAAE;MACnBC,MAAM,EAAE;IACV;EACF,CAAC;EAED,MAAMC,OAAOA,CAAA,EAAG;IACd,IAAIC,IAAG,GAAIC,YAAY,CAACC,OAAO,CAAC,WAAW,CAAC;IAC5C,IAAI,CAACF,IAAI,EAAE;MACT;MACA,IAAI,CAACG,UAAU,CAAC;QAAEC,GAAG,EAAE;MAAS,CAAC,CAAC;IACpC,OAAO;MACL,IAAI,CAACR,QAAO,GAAIS,IAAI,CAACC,KAAK,CAACN,IAAI,CAAC,CAACN,IAAI;MACrC,IAAI,CAACI,MAAK,GAAIO,IAAI,CAACC,KAAK,CAACN,IAAI,CAAC,CAACO,EAAE;IACnC;IACA,IAAIC,MAAK,GAAI,MAAMhB,KAAK,CAACiB,GAAG,CAAE,0CAAyC,IAAI,CAACX,MAAO,EAAC,CAAC;IACrF,IAAIU,MAAM,CAACE,MAAK,IAAK,GAAE,IAAKF,MAAM,CAACb,IAAI,CAACgB,MAAK,GAAI,CAAC,EAAE;MAClDC,OAAO,CAACC,GAAG,CAACL,MAAM,CAACb,IAAI,CAAC;MACxB,IAAI,CAACE,eAAc,GAAIW,MAAM,CAACb,IAAI;IACpC;EACF,CAAC;EAEDmB,UAAU,EAAE;IACVxB,MAAM;IACNG;EACF,CAAC;EAEDsB,OAAO,EAA