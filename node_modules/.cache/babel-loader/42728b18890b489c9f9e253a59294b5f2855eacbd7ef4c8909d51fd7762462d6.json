{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport Navbar from '@/components/header/Navbar.vue';\nimport { mapActions } from 'vuex';\nimport axios from 'axios';\nexport default {\n  name: 'DeleteAllLocations',\n  data() {\n    return {\n      userId: \"\",\n      allLocationsIds: [],\n      successMessage: '',\n      errorMessage: '',\n      allCatsIdIs: [],\n      allItemsIdIs: []\n    };\n  },\n  async mounted() {\n    let user = localStorage.getItem(\"user_info\");\n    if (user) {\n      this.userId = JSON.parse(user).id;\n\n      // All Locations\n      // http://localhost:3000/locations?userId=2\n      let result = await axios.get(`http://localhost:3000/locations?userId=${this.userId}`);\n      // console.log(result.data.length);\n      let resultLength = result.data.length;\n      for (let i = 0; i < resultLength; i++) {\n        this.allLocationsIds.push(result.data[i].id);\n      }\n      // console.table(this.allLocationsIds);\n\n      // All Categories\n      // http://localhost:3000/categories?userId=2\n      let resultCat = await axios.get(`http://localhost:3000/categories?userId=${this.userId}`);\n      let resultCatLength = resultCat.data.length;\n      for (var i = 0; i < resultCatLength; i++) {\n        this.allCatsIdIs.push(resultCat.data[i].id);\n      }\n\n      // All Items\n      // http://localhost:3000/items?userId=2\n      let resultItems = await axios.get(`http://localhost:3000/items?userId=${this.userId}`);\n      let resultItemsLength = resultItems.data.length;\n      for (var i = 0; i < resultItemsLength; i++) {\n        this.allItemsIdIs.push(resultItems.data[i].id);\n      }\n    } else {\n      // Redirect to Sign Up page\n      this.redirectTo({\n        val: 'signup'\n      });\n    }\n  },\n  components: {\n    Navbar\n  },\n  methods: {\n    ...mapActions(['redirectTo']),\n    // Redirect to Home Page\n    goBack() {\n      this.redirectTo({\n        val: \"home\"\n      });\n    },\n    async deleteAllRestaurants() {\n      // For Items\n      let allItemsResults = [];\n      for (var i = 0; i < this.allItemsIdIs.length; i++) {\n        let result = await axios.delete(`http://localhost:3000/items/${this.allItemsIdIs[i]}`);\n        if (result.status == 200) {\n          allItemsResults.push(true);\n        } else {\n          allItemsResults.push(false);\n        }\n      }\n\n      // For Categories\n      let allCatsResults = [];\n      for (var i = 0; i < this.allCatsIdIs.length; i++) {\n        let result = await axios.delete(`http://localhost:3000/categories/${this.allCatsIdIs[i]}`);\n        if (result.status == 200) {\n          allCatsResults.push(true);\n        } else {\n          allCatsResults.push(false);\n        }\n      }\n      let allResults = [];\n      for (let index = 0; index < this.allLocationsIds.length; index++) {\n        let result = await axios.delete(`http://localhost:3000/locations/${this.allLocationsIds[index]}`);\n        if (result.status == 200) {\n          allResults.push(true);\n        } else {\n          allResults.push(false);\n        }\n      }\n\n      // console.table(allResults);\n\n      if (!allResults.includes(false)) {\n        // Show Success Message\n        this.successMessage = 'Delete All Locations Successfully';\n        this.errorMessage = '';\n        setTimeout(() => {\n          this.redirectTo({\n            val: \"home\"\n          });\n        }, 2000);\n      } else {\n        // Show Error Message\n        this.successMessage = '';\n        this.errorMessage = 'Something went wrong, Try Again';\n      }\n    }\n  }\n};","map":{"version":3,"names":["Navbar","mapActions","axios","name","data","userId","allLocationsIds","successMessage","errorMessage","allCatsIdIs","allItemsIdIs","mounted","user","localStorage","getItem","JSON","parse","id","result","get","resultLength","length","i","push","resultCat","resultCatLength","resultItems","resultItemsLength","redirectTo","val","components","methods","goBack","deleteAllRestaurants","allItemsResults","delete","status","allCatsResults","allResults","index","includes","setTimeout"],"sources":["/opt/lampp/htdocs/rest-sys-2021/src/components/locations/DeleteAllLocations.vue"],"sourcesContent":["<template>\n\n    <div class=\"container\">\n\n        <Navbar />\n\n        <form @click.prevent>\n\n            <div class=\"row g-3 align-items-center\">\n                <div class=\"col-auto d-block mx-auto\">\n                    <h1 class=\"fs-5 text-center mb-3 text-danger\">Delete All Restaurants</h1>\n                    <p class=\"text-danger fw-bolder\">Are you sure you want to delete all locations?</p>\n                    <span class=\"text-muted\">It will also delete all related items and categories...., Be careful</span>\n                    <hr />\n                </div>\n            </div>\n\n            <div class=\"row g-3 align-items-center\">\n                <div class=\"col-auto d-block mx-auto\">\n                    <button type=\"button\" class=\"btn btn-info text-white\" @click=\"goBack()\">Go Back</button>\n                    &nbsp;\n                    <button type=\"button\" class=\"btn btn-danger text-white\" @click=\"deleteAllRestaurants()\">Delete All\n                        Now</button>\n                </div>\n            </div>\n\n            <br />\n\n            <div class=\"row g-3 align-items-center\">\n                <div class=\"col-auto d-block mx-auto alert alert-success\" v-if=\"successMessage.length > 0\">\n                    {{ successMessage }}\n                </div>\n                <div class=\"col-auto d-block mx-auto alert alert-danger\" v-if=\"errorMessage.length > 0\">\n                    {{ errorMessage }}\n                </div>\n            </div>\n\n        </form>\n\n    </div>\n\n</template>\n\n<script>\n\nimport Navbar from '@/components/header/Navbar.vue';\n\nimport { mapActions } from 'vuex';\n\nimport axios from 'axios';\n\nexport default {\n\n    name: 'DeleteAllLocations',\n\n    data() {\n        return {\n            userId: \"\",\n            allLocationsIds: [],\n            successMessage: '',\n            errorMessage: '',\n            allCatsIdIs: [],\n            allItemsIdIs: [],\n        }\n    },\n\n    async mounted() {\n\n        let user = localStorage.getItem(\"user_info\");\n\n        if (user) {\n\n            this.userId = JSON.parse(user).id;\n\n            // All Locations\n            // http://localhost:3000/locations?userId=2\n            let result = await axios.get(`http://localhost:3000/locations?userId=${this.userId}`);\n            // console.log(result.data.length);\n            let resultLength = result.data.length;\n            for (let i = 0; i < resultLength; i++) {\n                this.allLocationsIds.push(result.data[i].id);\n            }\n            // console.table(this.allLocationsIds);\n\n            // All Categories\n            // http://localhost:3000/categories?userId=2\n            let resultCat = await axios.get(`http://localhost:3000/categories?userId=${this.userId}`);\n            let resultCatLength = resultCat.data.length;\n            for (var i = 0; i < resultCatLength; i++) {\n                this.allCatsIdIs.push(resultCat.data[i].id);\n            }\n\n             // All Items\n            // http://localhost:3000/items?userId=2\n            let resultItems = await axios.get(`http://localhost:3000/items?userId=${this.userId}`);\n            let resultItemsLength = resultItems.data.length;\n            for (var i = 0; i < resultItemsLength; i++) {\n                this.allItemsIdIs.push(resultItems.data[i].id);\n            }\n\n        }\n        else {\n            // Redirect to Sign Up page\n            this.redirectTo({ val: 'signup' });\n        }\n    },\n\n    components: {\n        Navbar,\n    },\n\n    methods: {\n\n        ...mapActions(['redirectTo']),\n\n        // Redirect to Home Page\n        goBack() {\n            this.redirectTo({ val: \"home\" });\n        },\n\n        async deleteAllRestaurants() {\n\n            // For Items\n            let allItemsResults = [];\n            for (var i = 0; i < this.allItemsIdIs.length; i++) {\n                let result = await axios.delete(`http://localhost:3000/items/${this.allItemsIdIs[i]}`);\n\n                if (result.status == 200) {\n                    allItemsResults.push(true);\n                } else {\n                    allItemsResults.push(false);\n                }\n            }\n\n            // For Categories\n            let allCatsResults = [];\n            for (var i = 0; i < this.allCatsIdIs.length; i++) {\n                let result = await axios.delete(`http://localhost:3000/categories/${this.allCatsIdIs[i]}`);\n\n                if (result.status == 200) {\n                    allCatsResults.push(true);\n                } else {\n                    allCatsResults.push(false);\n                }\n            }\n\n            let allResults = [];\n\n            for (let index = 0; index < this.allLocationsIds.length; index++) {\n                let result = await axios.delete(`http://localhost:3000/locations/${this.allLocationsIds[index]}`);\n                if (result.status == 200) {\n                    allResults.push(true);\n                } else {\n                    allResults.push(false);\n                }\n            }\n\n            // console.table(allResults);\n\n            if (!allResults.includes(false)) {\n                // Show Success Message\n                this.successMessage = 'Delete All Locations Successfully';\n                this.errorMessage = '';\n                setTimeout(() => {\n                    this.redirectTo({ val: \"home\" });\n                }, 2000);\n            } else {\n                // Show Error Message\n                this.successMessage = '';\n                this.errorMessage = 'Something went wrong, Try Again';\n            }\n\n        },\n\n    },\n\n}\n\n</script>\n\n<style lang=\"scss\" scoped></style>"],"mappings":";AA6CA,OAAOA,MAAK,MAAO,gCAAgC;AAEnD,SAASC,UAAS,QAAS,MAAM;AAEjC,OAAOC,KAAI,MAAO,OAAO;AAEzB,eAAe;EAEXC,IAAI,EAAE,oBAAoB;EAE1BC,IAAIA,CAAA,EAAG;IACH,OAAO;MACHC,MAAM,EAAE,EAAE;MACVC,eAAe,EAAE,EAAE;MACnBC,cAAc,EAAE,EAAE;MAClBC,YAAY,EAAE,EAAE;MAChBC,WAAW,EAAE,EAAE;MACfC,YAAY,EAAE;IAClB;EACJ,CAAC;EAED,MAAMC,OAAOA,CAAA,EAAG;IAEZ,IAAIC,IAAG,GAAIC,YAAY,CAACC,OAAO,CAAC,WAAW,CAAC;IAE5C,IAAIF,IAAI,EAAE;MAEN,IAAI,CAACP,MAAK,GAAIU,IAAI,CAACC,KAAK,CAACJ,IAAI,CAAC,CAACK,EAAE;;MAEjC;MACA;MACA,IAAIC,MAAK,GAAI,MAAMhB,KAAK,CAACiB,GAAG,CAAE,0CAAyC,IAAI,CAACd,MAAO,EAAC,CAAC;MACrF;MACA,IAAIe,YAAW,GAAIF,MAAM,CAACd,IAAI,CAACiB,MAAM;MACrC,KAAK,IAAIC,CAAA,GAAI,CAAC,EAAEA,CAAA,GAAIF,YAAY,EAAEE,CAAC,EAAE,EAAE;QACnC,IAAI,CAAChB,eAAe,CAACiB,IAAI,CAACL,MAAM,CAACd,IAAI,CAACkB,CAAC,CAAC,CAACL,EAAE,CAAC;MAChD;MACA;;MAEA;MACA;MACA,IAAIO,SAAQ,GAAI,MAAMtB,KAAK,CAACiB,GAAG,CAAE,2CAA0C,IAAI,CAACd,MAAO,EAAC,CAAC;MACzF,IAAIoB,eAAc,GAAID,SAAS,CAACpB,IAAI,CAACiB,MAAM;MAC3C,KAAK,IAAIC,CAAA,GAAI,CAAC,EAAEA,CAAA,GAAIG,eAAe,EAAEH,CAAC,EAAE,EAAE;QACtC,IAAI,CAACb,WAAW,CAACc,IAAI,CAACC,SAAS,CAACpB,IAAI,CAACkB,CAAC,CAAC,CAACL,EAAE,CAAC;MAC/C;;MAEC;MACD;MACA,IAAIS,WAAU,GAAI,MAAMxB,KAAK,CAACiB,GAAG,CAAE,sCAAqC,IAAI,CAACd,MAAO,EAAC,CAAC;MACtF,IAAIsB,iBAAgB,GAAID,WAAW,CAACtB,IAAI,CAACiB,MAAM;MAC/C,KAAK,IAAIC,CAAA,GAAI,CAAC,EAAEA,CAAA,GAAIK,iBAAiB,EAAEL,CAAC,EAAE,EAAE;QACxC,IAAI,CAACZ,YAAY,CAACa,IAAI,CAACG,WAAW,CAACtB,IAAI,CAACkB,CAAC,CAAC,CAACL,EAAE,CAAC;MAClD;IAEJ,OACK;MACD;MACA,IAAI,CAACW,UAAU,CAAC;QAAEC,GAAG,EAAE;MAAS,CAAC,CAAC;IACtC;EACJ,CAAC;EAEDC,UAAU,EAAE;IACR9B;EACJ,CAAC;EAED+B,OAAO,EAAE;IAEL,GAAG9B,UAAU,CAAC,CAAC,YAAY,CAAC,CAAC;IAE7B;IACA+B,MAAMA,CAAA,EAAG;MACL,IAAI,CAACJ,UAAU,CAAC;QAAEC,GAAG,EAAE;MAAO,CAAC,CAAC;IACpC,CAAC;IAED,MAAMI,oBAAoBA,CAAA,EAAG;MAEzB;MACA,IAAIC,eAAc,GAAI,EAAE;MACxB,KAAK,IAAIZ,CAAA,GAAI,CAAC,EAAEA,CAAA,GAAI,IAAI,CAACZ,YAAY,CAACW,MAAM,EAAEC,CAAC,EAAE,EAAE;QAC/C,IAAIJ,MAAK,GAAI,MAAMhB,KAAK,CAACiC,MAAM,CAAE,+BAA8B,IAAI,CAACzB,YAAY,CAACY,CAAC,CAAE,EAAC,CAAC;QAEtF,