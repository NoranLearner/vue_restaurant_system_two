{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport Navbar from '@/components/header/Navbar.vue';\nimport { mapActions } from 'vuex';\nimport axios from 'axios';\nexport default {\n  name: 'DeleteAllLocations',\n  data() {\n    return {\n      userId: \"\",\n      allLocationsIds: [],\n      successMessage: '',\n      errorMessage: ''\n    };\n  },\n  async mounted() {\n    let user = localStorage.getItem(\"user_info\");\n    if (user) {\n      this.userId = JSON.parse(user).id;\n      let result = await axios.get(`http://localhost:3000/locations?userId=${this.userId}`);\n      // console.log(result.data.length);\n      let resultLength = result.data.length;\n      for (let i = 0; i < resultLength; i++) {\n        this.allLocationsIds.push(result.data[i].id);\n      }\n      // console.table(this.allLocationsIds);\n    } else {\n      // Redirect to Sign Up page\n      this.redirectTo({\n        val: 'signup'\n      });\n    }\n  },\n  components: {\n    Navbar\n  },\n  methods: {\n    ...mapActions(['redirectTo']),\n    // Redirect to Home Page\n    goBack() {\n      this.redirectTo({\n        val: \"home\"\n      });\n    },\n    async deleteAllRestaurants() {\n      let allResults = [];\n      for (let index = 0; index < this.allLocationsIds.length; index++) {\n        let result = await axios.delete(`http://localhost:3000/locations/${this.allLocationsIds[index]}`);\n        if (result.status == 200) {\n          allResults.push(true);\n        } else {\n          allResults.push(false);\n        }\n      }\n\n      // console.table(allResults);\n\n      if (!allResults.includes(false)) {\n        // Show Success Message\n        this.successMessage = 'Delete All Locations Successfully';\n        this.errorMessage = '';\n        setTimeout(() => {\n          this.redirectTo({\n            val: \"home\"\n          });\n        }, 2000);\n      } else {\n        // Show Error Message\n        this.successMessage = '';\n        this.errorMessage = 'Something went wrong, Try Again';\n      }\n    }\n  }\n};","map":{"version":3,"names":["Navbar","mapActions","axios","name","data","userId","allLocationsIds","successMessage","errorMessage","mounted","user","localStorage","getItem","JSON","parse","id","result","get","resultLength","length","i","push","redirectTo","val","components","methods","goBack","deleteAllRestaurants","allResults","index","delete","status","includes","setTimeout"],"sources":["/opt/lampp/htdocs/rest-sys-2021/src/components/locations/DeleteAllLocations.vue"],"sourcesContent":["<template>\n\n    <div class=\"container\">\n\n        <Navbar />\n\n        <form @click.prevent>\n\n            <div class=\"row g-3 align-items-center\">\n                <div class=\"col-auto d-block mx-auto\">\n                    <h1 class=\"fs-5 text-center mb-3 text-danger\">Delete All Restaurants</h1>\n                    <p class=\"text-danger fw-bolder\">Are you sure you want to delete all locations?</p>\n                    <span class=\"text-muted\">It will also delete all related items and categories...., Be careful</span>\n                    <hr />\n                </div>\n            </div>\n\n            <div class=\"row g-3 align-items-center\">\n                <div class=\"col-auto d-block mx-auto\">\n                    <button type=\"button\" class=\"btn btn-info text-white\" @click=\"goBack()\">Go Back</button>\n                    &nbsp;\n                    <button type=\"button\" class=\"btn btn-danger text-white\" @click=\"deleteAllRestaurants()\">Delete All\n                        Now</button>\n                </div>\n            </div>\n\n            <br />\n\n            <div class=\"row g-3 align-items-center\">\n                <div class=\"col-auto d-block mx-auto alert alert-success\" v-if=\"successMessage.length > 0\">\n                    {{ successMessage }}\n                </div>\n                <div class=\"col-auto d-block mx-auto alert alert-danger\" v-if=\"errorMessage.length > 0\">\n                    {{ errorMessage }}\n                </div>\n            </div>\n\n        </form>\n\n    </div>\n\n</template>\n\n<script>\n\nimport Navbar from '@/components/header/Navbar.vue';\n\nimport { mapActions } from 'vuex';\n\nimport axios from 'axios';\n\nexport default {\n\n    name: 'DeleteAllLocations',\n\n    data() {\n        return {\n            userId: \"\",\n            allLocationsIds: [],\n            successMessage: '',\n            errorMessage: '',\n        }\n    },\n\n    async mounted() {\n\n        let user = localStorage.getItem(\"user_info\");\n\n        if (user) {\n            this.userId = JSON.parse(user).id;\n            let result = await axios.get(`http://localhost:3000/locations?userId=${this.userId}`);\n            // console.log(result.data.length);\n            let resultLength = result.data.length;\n            for (let i = 0; i < resultLength; i++) {\n                this.allLocationsIds.push(result.data[i].id);\n            }\n            // console.table(this.allLocationsIds);\n        }\n        else {\n            // Redirect to Sign Up page\n            this.redirectTo({ val: 'signup' });\n        }\n    },\n\n    components: {\n        Navbar,\n    },\n\n    methods: {\n\n        ...mapActions(['redirectTo']),\n\n        // Redirect to Home Page\n        goBack() {\n            this.redirectTo({ val: \"home\" });\n        },\n\n        async deleteAllRestaurants() {\n\n            let allResults = [];\n\n            for (let index = 0; index < this.allLocationsIds.length; index++) {\n                let result = await axios.delete(`http://localhost:3000/locations/${this.allLocationsIds[index]}`);\n                if (result.status == 200) {\n                    allResults.push(true);\n                } else {\n                    allResults.push(false);\n                }\n            }\n\n            // console.table(allResults);\n\n            if (!allResults.includes(false)) {\n                // Show Success Message\n                this.successMessage = 'Delete All Locations Successfully';\n                this.errorMessage = '';\n                setTimeout(() => {\n                    this.redirectTo({ val: \"home\" });\n                }, 2000);\n            } else {\n                // Show Error Message\n                this.successMessage = '';\n                this.errorMessage = 'Something went wrong, Try Again';\n            }\n\n        },\n\n    },\n\n}\n\n</script>\n\n<style lang=\"scss\" scoped></style>"],"mappings":";AA6CA,OAAOA,MAAK,MAAO,gCAAgC;AAEnD,SAASC,UAAS,QAAS,MAAM;AAEjC,OAAOC,KAAI,MAAO,OAAO;AAEzB,eAAe;EAEXC,IAAI,EAAE,oBAAoB;EAE1BC,IAAIA,CAAA,EAAG;IACH,OAAO;MACHC,MAAM,EAAE,EAAE;MACVC,eAAe,EAAE,EAAE;MACnBC,cAAc,EAAE,EAAE;MAClBC,YAAY,EAAE;IAClB;EACJ,CAAC;EAED,MAAMC,OAAOA,CAAA,EAAG;IAEZ,IAAIC,IAAG,GAAIC,YAAY,CAACC,OAAO,CAAC,WAAW,CAAC;IAE5C,IAAIF,IAAI,EAAE;MACN,IAAI,CAACL,MAAK,GAAIQ,IAAI,CAACC,KAAK,CAACJ,IAAI,CAAC,CAACK,EAAE;MACjC,IAAIC,MAAK,GAAI,MAAMd,KAAK,CAACe,GAAG,CAAE,0CAAyC,IAAI,CAACZ,MAAO,EAAC,CAAC;MACrF;MACA,IAAIa,YAAW,GAAIF,MAAM,CAACZ,IAAI,CAACe,MAAM;MACrC,KAAK,IAAIC,CAAA,GAAI,CAAC,EAAEA,CAAA,GAAIF,YAAY,EAAEE,CAAC,EAAE,EAAE;QACnC,IAAI,CAACd,eAAe,CAACe,IAAI,CAACL,MAAM,CAACZ,IAAI,CAACgB,CAAC,CAAC,CAACL,EAAE,CAAC;MAChD;MACA;IACJ,OACK;MACD;MACA,IAAI,CAACO,UAAU,CAAC;QAAEC,GAAG,EAAE;MAAS,CAAC,CAAC;IACtC;EACJ,CAAC;EAEDC,UAAU,EAAE;IACRxB;EACJ,CAAC;EAEDyB,OAAO,EAAE;IAEL,GAAGxB,UAAU,CAAC,CAAC,YAAY,CAAC,CAAC;IAE7B;IACAyB,MAAMA,CAAA,EAAG;MACL,IAAI,CAACJ,UAAU,CAAC;QAAEC,GAAG,EAAE;MAAO,CAAC,CAAC;IACpC,CAAC;IAED,MAAMI,oBAAoBA,CAAA,EAAG;MAEzB,IAAIC,UAAS,GAAI,EAAE;MAEnB,KAAK,IAAIC,KAAI,GAAI,CAAC,EAAEA,KAAI,GAAI,IAAI,CAACvB,eAAe,CAACa,MAAM,EAAEU,KAAK,EAAE,EAAE;QAC9D,IAAIb,MAAK,GAAI,MAAMd,KAAK,CAAC4B,MAAM,CAAE,mCAAkC,IAAI,CAACxB,eAAe,CAACuB,KAAK,CAAE,EAAC,CAAC;QACjG,IAAIb,MAAM,CAACe,MAAK,IAAK,GAAG,EAAE;UACtBH,UAAU,CAACP,IAAI,CAAC,IAAI,CAAC;QACzB,OAAO;UACHO,UAAU,CAACP,IAAI,CAAC,KAAK,CAAC;QAC1B;MACJ;;MAEA;;MAEA,IAAI,CAACO,UAAU,CAACI,QAAQ,CAAC,KAAK,CAAC,EA